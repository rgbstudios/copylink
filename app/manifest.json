{
	"manifest_version": 2,
	"name": "Copylink",
	"description": "Click to copy the url of the current page to your clipboard",
	"version": "1.2.0",
	"browser_action": {
		"default_icon": "128_icon.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"clipboardWrite"
	],
	"icons": {
		"128": "128_icon.png",
		"48": "48_icon.png",
		"16": "16_icon.png"
	}
}